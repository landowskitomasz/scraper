<shop>
	<category name="mens-tennis-shoes-nike"
	    url="http://www.directtennis.co.uk/Section.aspx?Category=Direct-Tennis&amp;Dept=mens-tennis-shoes&amp;Section=nike&amp;NavID=41">
    	<dataEntry name="product" xPath="/html/body/form[@id='form1']/div/div[@id='MainDiv']/div[@id='MainContentHolderDiv']/div[@id='SectionHolderDiv']/span[@id='SectionCentreNav1']/span/div[@class='sectionNavMainDiv']">
    	    <field name="name" xPath="div[@class='sectionNavNameDiv']/span[@class='productAnchor']/a" >
    	        <valueProcessor processorType="htmlUnescape" />
    	    </field>
    	    <field name="price" xPath="div[@class='sectionNavTextDiv']/div[@class='ProductListingsOURPriceW']">
            	<valueProcessor processorType="regexClean">
                	<option name="regex" value="[a-zA-Z $\s\?]"/>
				</valueProcessor>
                <valueProcessor processorType="validate" >
                	<option name="regex" value="^[0-9]*[.][0-9]+$"/>
                </valueProcessor>
            </field>
            <field name="url" xPath="div[@class='sectionNavNameDiv']/span[@class='productAnchor']/a/@href" />
		</dataEntry>
	</category>
</shop>